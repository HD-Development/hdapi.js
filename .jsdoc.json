{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "readme": "README.md",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/docdash"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
     }, 
     "docdash": {
        "static": false, 
        "short": true,
        "meta": {
            "title": "hdapi.js documentation", 
            "description": "", 
            "keywords": "hdapi.js, bot, discord bot, discord"
        }
     }
}